{
  "name": "http-proxy",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "vercel-dev": "vercel dev --debug",
    "build-page": "node build-cloudflare-page.js",
    "build-worker": "node build-cloudflare-worker.js",
    "build-deno": "deno run -A build-deno-deploy.ts",
    "build-fastly": "js-compute-runtime fastly-entry.js bin/main.wasm",
    "start-fastly": "fastly compute serve",
    "deploy-fastly": "dotenv -- fastly compute publish"
  },
  "packageManager": "pnpm@10.12.2",
  "dependencies": {
    "@fastly/js-compute": "^3.34.0"
  },
  "devDependencies": {
    "dotenv-cli": "^10.0.0"
  }
}
